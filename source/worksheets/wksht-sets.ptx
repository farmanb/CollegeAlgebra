<?xml version="1.0" encoding="UTF-8"?>
<worksheet xml:id="wksht-working_with_sets" component="notes">
  <title>Worksheet: Sets</title>
  <objectives>
    <ul>
      <li>
	<p>
	  Use roster and set-builder notation to express a set,
	</p>
      </li>
      <li>
	<p>
	  Find the intersection and union of two sets.
	</p>
      </li>
    </ul>
  </objectives>
  <page>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write the real numbers that are at least as large as 2 and less than 5 in set notation.
	</p>
      </statement>
    </exercise>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write the integers that are at least as large as 2 and less than 5 in set notation.
	</p>
      </statement>
    </exercise>
  </page>
  <page>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write <m>[0,\infty) \cap (-\infty, 5)</m> in set notation.
	  Graph the result on a number line.
	</p>
      </statement>
    </exercise>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write <m>[0,5) \cap (3, 5)</m> in set notation.
	  Graph the result on a number line.
	</p>
      </statement>
    </exercise>
  </page>
  <page>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write <m>[0,5) \cup (3, 5)</m> in set notation.
	  Graph the result on a number line.
	</p>
      </statement>
    </exercise>
    <exercise workspace="0.5in">
      <statement>
	<p>
	  Write <m>[0,\infty) \cup (-\infty, 5)</m> in set notation.
	  Graph the result on a number line.
	</p>
      </statement>
    </exercise>
  </page>
</worksheet>
