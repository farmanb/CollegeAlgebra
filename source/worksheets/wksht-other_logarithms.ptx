<?xml version="1.0" encoding="UTF-8"?>
<worksheet xml:id="wksht-other_logs" xmlns:xi="http://www.w3.org/2001/XInclude" component="notes">
  <title>Worksheet: Other Logarithmic Functions</title>
  <objectives>
    <ul>
      <li>
	Solve equations involving exponential functions.
      </li>
      <li>
	Solve equations involving logarithms.
      </li>
    </ul>
  </objectives>
  <page>
    <title>Solving Equations with Exponential Functions</title>
    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation
	  <me>\Exp{7}(2x + 1) = 49</me>
	  for <m>x</m>.
	</p>
      </statement>
    </exercise>
    
    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation 
	  <me>\Exp{2}(-4x) = 16 \Exp{2}\left({x^2}\right)</me>
	  for <m>x</m>.
	</p>
      </statement>
    </exercise>

    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation
	  <me>\Exp{\frac{1}{2}}(x^2) = \Exp{\frac{1}{2}}(−2x−1)</me>
	  for <m>x</m>
	</p>
      </statement>
    </exercise>
  </page>
  <page>
    <title>Solving Equations with Logarithms</title>
    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation
	  <me>\log_4(x + 3) + \log_4(x − 3) = 2</me>
	  for <m>x</m>.
	</p>
      </statement>
    </exercise>
    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation
	  <me>\log_6(x - 5) - 2\log_6\left(\frac{1}{\sqrt{x}}\right) = 1.</me>
	  for <m>x</m>.
	</p>
      </statement>
    </exercise>
    <exercise workspace="2in">
      <statement>
	<p>
	  Solve the equation
	  <me>-\log_2\left(\frac{1}{5-x}\right)+2\log_2\left(\sqrt{x+1}\right)=2</me>
	  for <m>x</m>.
	</p>
      </statement>
    </exercise>
  </page>
</worksheet>
