<?xml version="1.0" encoding="UTF-8"?>
<worksheet xml:id="wksht-rational_functions" component="notes">
  <title>Worksheet: Rational Functions</title>
  <objectives>
    <ul>
      <li>
	<p>
	  Combine rational functions
	</p>
      </li>
      <li>
	<p>
	  Solve rational inequalities
	</p>
      </li>
    </ul>
  </objectives>

  <introduction>
    <p>
      Solve the following rational inequalities.
    </p>
  </introduction>

  <page>
    <exercise workspace="3in">
      <statement>
	<p>
	  <m>\dfrac{2x - 1}{3} > 5</m>
	</p>
      </statement>
    </exercise>

    <exercise workspace="3in">
      <statement>
	<p>
	  <m>\dfrac{x^2 - 4}{x + 2} \leq 0</m>
	</p>
      </statement>
    </exercise>
  </page>

  <page>
    <exercise workspace="4in">
      <statement>
	<p>
	  <m>\dfrac{3x - 2}{x + 1} \lt 4</m>
	</p>
      </statement>
    </exercise>

    <exercise workspace="4in">
      <statement>
	<p>
	  <m>\dfrac{2x + 1}{x - 3} \geq \dfrac{x + 4}{2}</m>
	</p>
      </statement>
    </exercise>
  </page>

  <page>
    <exercise workspace="4in">
      <statement>
	<p>
	  <m>\dfrac{x - 1}{x^2 + 3x + 2} \lt 0</m>
	</p>
      </statement>
    </exercise>

    <exercise workspace="4in">
      <statement>
	<p>
	  <m>\dfrac{2x^2 - 5x + 3}{x^2 - x - 6} \geq \dfrac{x + 2}{x^2 - 4}</m>
	</p>
      </statement>
    </exercise>
  </page>
</worksheet>
